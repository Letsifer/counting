<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/3.0/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/3.0/modelMap http://cayenne.apache.org/schema/3.0/modelMap.xsd"
	 project-version="6">
	<property name="defaultPackage" value="ru.lets.counting.domain"/>
	<db-entity name="categories" schema="counting">
		<db-attribute name="description" type="VARCHAR" length="2147483647"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="10"/>
		<db-attribute name="parent_category_id" type="INTEGER" length="10"/>
		<db-attribute name="title" type="VARCHAR" isMandatory="true" length="2147483647"/>
	</db-entity>
	<db-entity name="purchases" schema="counting">
		<db-attribute name="amount" type="INTEGER" isMandatory="true" length="10"/>
		<db-attribute name="category_id" type="INTEGER" isMandatory="true" length="10"/>
		<db-attribute name="description" type="VARCHAR" length="2147483647"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="10"/>
	</db-entity>
	<obj-entity name="Category" className="ru.lets.counting.domain.Category" dbEntityName="categories">
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
		<obj-attribute name="title" type="java.lang.String" db-attribute-path="title"/>
	</obj-entity>
	<obj-entity name="Purchase" className="ru.lets.counting.domain.Purchase" dbEntityName="purchases">
		<obj-attribute name="amount" type="java.lang.Integer" db-attribute-path="amount"/>
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
	</obj-entity>
	<db-relationship name="categoriesArray" source="categories" target="categories" toMany="true">
		<db-attribute-pair source="id" target="parent_category_id"/>
	</db-relationship>
	<db-relationship name="purchasesArray" source="categories" target="purchases" toMany="true">
		<db-attribute-pair source="id" target="category_id"/>
	</db-relationship>
	<db-relationship name="toCategories" source="categories" target="categories" toMany="false">
		<db-attribute-pair source="parent_category_id" target="id"/>
	</db-relationship>
	<db-relationship name="toCategories" source="purchases" target="categories" toMany="false">
		<db-attribute-pair source="category_id" target="id"/>
	</db-relationship>
	<obj-relationship name="categoriesArray" source="Category" target="Category" deleteRule="Deny" db-relationship-path="categoriesArray"/>
	<obj-relationship name="purchasesArray" source="Category" target="Purchase" deleteRule="Deny" db-relationship-path="purchasesArray"/>
	<obj-relationship name="toCategories" source="Category" target="Category" deleteRule="Nullify" db-relationship-path="toCategories"/>
	<obj-relationship name="toCategories" source="Purchase" target="Category" deleteRule="Nullify" db-relationship-path="toCategories"/>
</data-map>
